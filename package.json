{
  "name": "caly",
  "version": "0.7.2",
  "description": "Caly is a small calendar!",
  "keywords": [
    "calendar",
    "stencil"
  ],
  "main": "./dist/index.cjs.js",
  "module": "./dist/caly/caly.esm.js",
  "types": "./dist/types/index.d.ts",
  "collection": "./dist/collection/collection-manifest.json",
  "collection:main": "./dist/collection/index.js",
  "unpkg": "./dist/caly/caly.esm.js",
  "files": [
    "dist/",
    "loader/"
  ],
  "exports": {
    "import": "./dist/caly/caly.esm.js",
    "require": "./dist/index.cjs.js"
  },
  "scripts": {
    "build": "stencil build --docs",
    "start": "stencil build --dev --watch --serve",
    "test": "stencil test --spec --e2e",
    "test.watch": "stencil test --spec --e2e --watchAll"
  },
  "devDependencies": {
    "@stencil/core": "^2.1.1",
    "@types/jest": "26.0.15",
    "@types/puppeteer": "3.0.4",
    "cntdys": "^0.5.1",
    "jest": "26.6.1",
    "jest-cli": "26.6.1",
    "puppeteer": "5.4.1"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/zigomir/caly.git"
  },
  "homepage": "https://github.com/zigomir/caly",
  "author": "Ziga Vidic <ziga@hey.com> (https://ziga.dev/)",
  "license": "MIT"
}
